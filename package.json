{
  "name": "my-qwik-empty-starter",
  "description": "Blank project with routing included",
  "engines": {
    "node": "^18.17.0 || ^20.3.0 || >=21.0.0"
  },
  "engines-annotation": "Mostly required by sharp which needs a Node-API v9 compatible runtime",
  "private": true,
  "trustedDependencies": [
    "sharp"
  ],
  "trustedDependencies-annotation": "Needed for bun to allow running install scripts",
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "qwik": "qwik",
    "db:models": "node db/scripts/generate-models.js",
    "db:sync": "node db/scripts/sync.js",
    "db:seed": "node db/scripts/seed.js",
    "db:docs": "npx dbdocs build db/db.dbml"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.7.1",
    "@builder.io/qwik-city": "^1.7.1",
    "@types/eslint": "^8.56.10",
    "@types/node": "^20.14.10",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "eslint": "^8.57.0",
    "eslint-plugin-qwik": "^1.7.1",
    "prettier": "^3.3.3",
    "typescript": "5.4.5",
    "undici": "^6.19.2",
    "vite": "^5.3.3",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "dependencies": {
    "@hono/swagger-ui": "^0.4.0",
    "@hono/zod-openapi": "^0.14.9",
    "@types/moment": "^2.13.0",
    "@types/shelljs": "^0.8.15",
    "bcryptjs": "^2.4.3",
    "dockerode": "^4.0.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "fast-csv": "^5.0.1",
    "hono": "^4.4.13",
    "js-cookie": "^3.0.5",
    "json-schema-to-zod": "^2.3.1",
    "jsonwebtoken": "^9.0.2",
    "module-from-string": "^3.3.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "mysql2": "^3.10.2",
    "pg-hstore": "^2.3.4",
    "recursive-iterator": "^3.3.0",
    "sass": "^1.77.8",
    "sequelize": "^6.37.3",
    "sequelize-auto": "^0.8.8",
    "sequelize-cli": "^6.6.2",
    "shelljs": "^0.8.5",
    "sqlite3": "^5.1.7",
    "workerpool": "^9.1.3",
    "zod": "^3.23.8"
  }
}
